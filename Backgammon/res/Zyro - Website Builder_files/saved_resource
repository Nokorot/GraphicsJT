		wb_builder.showPremiumBtn = false;
		wb_builder.isZyro = true;
		
		wb_builder.isB2CFree = false;
		wb_builder.formsAllowed = true;
		wb_builder.autoBackupsEnable = false;
		wb_builder.autoBackupsSavingInterval = 600;
